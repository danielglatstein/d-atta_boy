<button id="update_players">Update Players</button> 
<form id="matchup_form">
  <select id="pitcher_id">
    <option value="choose_pitcher">Choose A Pitcher</option>
    <% Player.pitchers.each do |pitcher| %>
      <option value="<%=pitcher%>"> <%= "#{Player.find_by(player_id: pitcher).first} #{Player.find_by(player_id: pitcher).last}" %> </option> %>
    <% end %>
  </select>
  <select id="batter_id">
    <option>Select Batter</option>
  </select>
</form>