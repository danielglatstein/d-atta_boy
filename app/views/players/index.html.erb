<h3>D-atta Boy</h3>

<p>2015 Playoffs Roster</p>

<ul>
  $("#player-list").append("<li>Appended item</li>");
</ul>


<div id='users-counter'>
  <%= @players %>
</div>

<a href='/players' id='stop-renewing'>
</a>


<table id="split">
  <thead></thead>
  <tbody></tbody>
</table>

    <script type="text/javascript">
      var sessions = gon.watch.players;
      
      // create the table header
      var thead = d3.select("thead").selectAll("th").data(d3.keys(sessions[0])).enter().append("th").text(function(d){return d});
      // fill the table
      // create rows
      var tr = d3.select("tbody").selectAll("tr").data(sessions).enter().append("tr")
      // cells
      var td = tr.selectAll("td").data(function(d){return d3.values(d)}).enter().append("td").text(function(d) {return d})
    </script>